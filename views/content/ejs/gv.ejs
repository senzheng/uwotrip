   <div class="container-fluid content">
        <div class="slider">
          <img src="/img/LA1.jpg">
        </div>
        <div class="profile">
          <div class="left col-md-6">
                <div class="userpic2">
                  <img src="/img/guides/lasvegas/photos/hY1D3G0nPq.jpg">
                </div>
                <div class="name"><%= guides[0].last_name %> <%= guides[0].first_name %></div>
                <div class="rating"><uib-rating ng-model="total" max="max" read-only="isReadonly" on-hover="hoveringOver(value)" on-leave="overStar = null" titles="['one','two','three']" aria-labelledby="default-rating" style="float: left" disabled></uib-rating><input type="text" name="rating" value="{{total}}" style="display:none"/><span class="reviewamount"> 1 Reviews</span></div>
                
                <div class="intro">
                    <h5>INTRODUCTION</h5>
                    <p><%= guides[0].description %></p>
                </div>
          </div>
          <div class="right col-md-6">
                <div class="icon">
                  <div class="icon-left"><i class="fa fa-heart-o fa-lg"></i></div>
                  <div class="icon-right"><i class="fa fa-envelope-o fa-lg"></i></div>
                </div>
                <div class="contentwo"><%= guides[0].description %>
                </div>
                <div class="reviews">
                  <div class="pangination">
                    <span>REVIEWS</span>
                       <ul>
                         <li><button>1</button></li>
                       </ul>
                  </div>
                  
                    <div class="row">
                      <div class="col-md-3">
                          <div class="userpic">
                            <img src="">
                          </div>
                      </div>
                       <div class="col-md-9">
                            <ul>
                              <li>Name:</li>
                              <li><uib-rating ng-model="total" max="max" read-only="isReadonly" on-hover="hoveringOver(value)" on-leave="overStar = null" titles="['one','two','three']" aria-labelledby="default-rating" style="float: left" disabled></uib-rating><input type="text" name="rating" value="{{total}}" style="display:none"/>  3/10/16 </li>
                              <li>Good indeed </li>
                            </ul>
                      </div>
                  
                </div>
          </div>
        </div>


   </div>


   
