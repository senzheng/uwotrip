<body ng-controller="guideCtrl" style="padding: 0px;">
     <nav class="navbar navbar-inverse" style="background-color: #8EBFE7; border-radius: 0px; margin-bottom: 0px;">
  <div class="container-fluid">
    <!-- Brand and toggle get grouped for better mobile display -->
